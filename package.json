{
  "name": "resume-template",
  "version": "1.0.0",
  "description": "A professional resume website template with JSON configuration and CLI scaffolding",
  "main": "server.js",
  "bin": {
    "create-resume": "./bin/create-resume.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node build-resume.js",
    "build:watch": "node build-resume.js && nodemon --watch resume-data.json --exec 'npm run build'",
    "deploy": "npm run build && gh-pages -d public",
    "create": "node bin/create-resume.js"
  },
  "keywords": [
    "resume",
    "portfolio",
    "template",
    "cli",
    "generator",
    "json-driven"
  ],
  "author": "Abhishek Patro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/springbootMicroservices2/resume-website"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "gh-pages": "^6.1.0",
    "nodemon": "^3.0.1",
    "rimraf": "^6.1.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
